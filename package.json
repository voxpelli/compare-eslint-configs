{
  "name": "compare-eslint-configs",
  "version": "1.1.0",
  "description": "Compares ESLint configs",
  "homepage": "http://github.com/voxpelli/compare-eslint-configs",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/compare-eslint-configs.git"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "type": "module",
  "bin": {
    "compare-eslint-configs": "cli.js"
  },
  "files": [
    "cli.js",
    "lib/**/*.js"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check '*.js' 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check -i eslint-plugin-jsdoc",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 95 --ignore-files 'test/*'",
    "check": "run-s clean && run-p -c --aggregate-output check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts*')",
    "clean": "run-p clean:*",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*",
    "try": "node cli.js new.eslintrc -t cli.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/eslint": "^8.4.6",
    "@types/mocha": "^10.0.0",
    "@types/node": "^14.18.31",
    "@voxpelli/eslint-config": "^15.2.0",
    "@voxpelli/tsconfig": "^4.0.0",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "dependency-check": "^5.0.0-7",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "installed-check": "^6.0.4",
    "mocha": "^10.0.0",
    "npm-run-all2": "^6.0.2",
    "type-coverage": "^2.21.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "chalk": "^5.1.0",
    "eslint": "^8.25.0",
    "fast-deep-equal": "^3.1.3",
    "is-unicode-supported": "^1.3.0",
    "meow": "^11.0.0",
    "pony-cause": "^2.1.4",
    "terminal-link": "^3.0.0"
  }
}
